<div class="rentals">
    <button type="button" class="button1 up" {{on 'click' (fn this.avgSal @employees)}}>
        Get Average Salary
    </button>
    <NewEmployee @company_id={{@companyId}}/>
    <ul class="results">
        {{#each @employees as |employee|}}
            <li><Employee @employee={{employee}} /></li>
        {{else}}
            No employees exist.
        {{/each}}
    </ul>
</div>